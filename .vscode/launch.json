	"version": "0.2.0",
	"configurations": [
		{
			"name": "tester",
			"program": "${workspaceFolder}/create-em.js",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "pwa-node"
		},
		{
			"name": "debug:riktest",
			"request": "launch",
			"runtimeArgs": [
				"run-script",
				"riktest"
			],
			"runtimeExecutable": "npm",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "pwa-node"
		},
		{
			"name": "debug:create-em",
			"request": "launch",
			"runtimeArgs": [
				"run-script",
				"create-em"
			],
			"runtimeExecutable": "npm",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "pwa-node"
		}
	]
}